<p id="notice"><%= notice %></p>

<h1>Analysis Configurations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_configurations.each do |analysis_configuration| %>
      <tr>
        <td><%= link_to 'Show', analysis_configuration %></td>
        <td><%= link_to 'Edit', edit_analysis_configuration_path(analysis_configuration) %></td>
        <td><%= link_to 'Destroy', analysis_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Configuration', new_analysis_configuration_path %>
